msgid "Week"
msgstr "सप्ताह"

#: visualfreebusywidget.cpp:157
#, kde-format
msgctxt "@item:inlistbox range in months"
msgid "Month"
msgstr "महीना"

#: visualfreebusywidget.cpp:158
#, kde-format
msgctxt "@item:inlistbox range is computed automatically"
msgid "Automatic"
msgstr "स्वचालित"

#: visualfreebusywidget.cpp:163
